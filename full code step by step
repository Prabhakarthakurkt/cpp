/**virtual function***/
#include<iostream>
using namespace std;
class your
class my
{
private :
    int a;
public :
    int c;

    friend your;

};
class your
{
public:
    my m;
    void fun()
    {
        m.a=10;
        m.b=10;
        m.c=10;
    }
};
int main()
{



}
