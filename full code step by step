//friend operator overlod
/*
#include<iostream>
using namespace std;
class Complex
{
  private:
    int real;
    int img;
  public:
    complex(int r=0,int i=0)
    {
        real=r;
        img=i;
    }
    void display()
    {
        cout<< "+i"<<img<<endl;
    }

    friend ostream & operator<<(ostream &out,Complex &c)
};
