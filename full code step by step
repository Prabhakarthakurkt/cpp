/*
#include<iostream>
using namespace std;

int main()
{
    int n,num,digit, rev = 0;

    cout<< "Enter a postitive number:";
    cin>>num;

    n = num;

    do {
        digit = num % 10;
        rev = (rev * 10) +digit;
        num = num/100;
    }while(num !=0);

    cout<< "the reverse of the number is:"<<rev<<endl;

    if(n == rev)
        cout << "The number is a palindrome";
    else
        cout<< "The number is not a palindrome";

    return 0;
} */
